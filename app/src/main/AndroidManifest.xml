<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.indiancalendar.hindicalendar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.indiancalendar.hindicalendar.Splash_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.indiancalendar.hindicalendar.About_us" />
        <activity android:name="com.indiancalendar.hindicalendar.Festival" />
        <activity android:name="com.indiancalendar.hindicalendar.Horoscope" />
        <activity android:name="com.indiancalendar.hindicalendar.Vivah_Muhurt" />
        <activity android:name="com.indiancalendar.hindicalendar.HolidayCalender" />
        <activity
            android:name="com.indiancalendar.hindicalendar.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <!-- Sample AdMob App ID: ca-app-pub-3940256099942544~3347511713 -->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-8544924075745696~1055092105"/>
    </application>

</manifest>